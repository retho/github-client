{
  "name": "github-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:strict": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "pretty": "pretty-quick src/**/*.{ts,tsx} --staged",
    "pretty:all": "prettier --write src/**/*.{ts,tsx}",
    "checklist": "npm run lint:strict && npm run stylelint && npm run test:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run checklist"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.5",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.8",
    "@reduxjs/toolkit": "^1.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "lodash-es": "^4.17.15",
    "normalize.css": "^8.0.1",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-switch": "^5.0.1",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "typescript": "~3.7.2",
    "url-pattern": "^1.0.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  }
}
